<launch>
  <arg name="nao_ip" default="$(optenv NAO_IP 10.0.1.207)" />
  <arg name="nao_port" default="$(optenv NAO_PORT 9559)" />
  <param name="nao_ip" type="string" value="$(arg nao_ip)"/>
  <param name="nao_port" type="string" value="$(arg nao_port)"/>
  
  <node pkg="pepper_nodes" type="voice_detection.py" name="voice_detection" required="true" args="--ip=$(arg nao_ip) --port=$(arg nao_port)" output="screen"/>
  <node pkg="pepper_nodes" type="asr.py" name="asr" required="true" args="--ip=$(arg nao_ip) --port=$(arg nao_port)" output="screen"/>
</launch>

